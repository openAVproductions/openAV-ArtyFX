@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix epp: <http://lv2plug.in/ns/ext/port-props#> .
@prefix ui: <http://lv2plug.in/ns/extensions/ui#>.
@prefix pprop:  <http://lv2plug.in/ns/ext/port-props#>.

<http://www.openavproductions.com/artyfx#kuiza/gui>
  a ui:X11UI;
  ui:binary <artyfx_ui.so>;
  lv2:optionalFeature ui:noUserResize ;
  lv2:extensionData   ui:idleInterface ;
  ui:requiredFeature ui:makeResident.

<http://www.openavproductions.com/artyfx#kuiza>
  a lv2:Plugin ,
    lv2:EQPlugin ;
  
  doap:name "Kuiza";
  doap:description "4-band Fixed Parameteric EQ";
  doap:maintainer  [
      foaf:homepage <http://openavproductions.com>; 
      foaf:mbox <mailto:harryhaaren@gmail.com>;
      foaf:name "OpenAV"
  ];
  lv2:optionalFeature lv2:hardRTCapable ;
  
  ui:ui <http://www.openavproductions.com/artyfx#kuiza/gui>;
  
  lv2:requiredFeature <http://lv2plug.in/ns/ext/urid#map> ;

  lv2:port [
      a lv2:InputPort, lv2:AudioPort ;
      lv2:index 0 ;
      lv2:symbol "input" ;
      lv2:name "Audio Input" ;
  ] , [
      a lv2:OutputPort, lv2:AudioPort ;
      lv2:index 1 ;
      lv2:symbol "output" ;
      lv2:name "Audio Output" ;
  ] , [
      a lv2:InputPort, lv2:ControlPort ;
      lv2:index 2 ;
      lv2:name "Gain" ;
      lv2:symbol "gain" ;
      lv2:default 0.500000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
  ] , [
      a lv2:InputPort, lv2:ControlPort ;
      lv2:index 3 ;
      lv2:name "Low Gain" ;
      lv2:symbol "gain_1" ;
      lv2:default 0.500000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
  ] , [
      a lv2:InputPort, lv2:ControlPort ;
      lv2:index 4 ;
      lv2:name "LoMid Gain" ;
      lv2:symbol "gain_2" ;
      lv2:default 0.500000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
  ] , [
      a lv2:InputPort, lv2:ControlPort ;
      lv2:index 5 ;
      lv2:name "HiMid Gain" ;
      lv2:symbol "gain_3" ;
      lv2:default 0.500000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
  ] , [
      a lv2:InputPort, lv2:ControlPort ;
      lv2:index 6 ;
      lv2:name "High Gain" ;
      lv2:symbol "gain_4" ;
      lv2:default 0.500000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
  ] , [
      a lv2:InputPort, lv2:ControlPort ;
      lv2:index 7 ;
      lv2:name "Active" ;
      lv2:symbol "active" ;
      lv2:default 1.000000 ;
      lv2:minimum 0.000000 ;
      lv2:maximum 1.000000 ;
      lv2:portProperty lv2:toggled ;
      lv2:portProperty pprop:notOnGUI ;
  ]
.
